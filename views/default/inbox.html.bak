{{extend 'layout.html'}}
<table style="width:60%">
    {{for i in one:}}
    <tr>
        <td style="width:50%"><a href="{{=URL('default','view_user',args=[i.me])}}">{{=i.me}}</a></td>
        <td style="width:50%"><button onclick='window.location="{{=URL('default','message',args=[i.me])}}"'>
        view
        </button></td>
    </tr>
    {{pass}}
</table>
