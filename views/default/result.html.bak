{{extend 'layout.html'}}
<!--
{{=SQLTABLE(rows)}}
-->
<style>
tr.spaceUnder > td {
   padding-bottom:1em;
}
</style>
    {{=len(rows)}}
    {{if len(rows)==0:}}
    {{=redirect(URL('default', 'notfound.html'))}}

<table style="font-size:200%; width:60%">
    <tr class="spaceUnder">
        <td>First Name</td>
        <td>Last Name</td>
        <td>Age</td>
        <td>salary</td>
    </tr>

    {{pass}}
    {{for i in rows:}}
<tr class="spaceUnder">
    <td><a href="{{=URL('default','view_user',args=[i.last_name])}}">{{=i.first_name}}</a></td>
    <td><a href="{{=URL('default','view_user',args=[i.last_name])}}">{{=i.last_name}}</a></td>
    <td>{{=i.age}}</td>
    <td>{{=i.salary}}</td>
</tr>
    {{pass}}
</table>
